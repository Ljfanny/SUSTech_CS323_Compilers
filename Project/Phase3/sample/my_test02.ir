FUNCTION fact :
PARAM v0
IF v0 == #1 GOTO label0
GOTO label1
LABEL label0 :
RETURN v0
LABEL label1 :
t0 := v0 - #1
ARG t0
t1 := CALL fact
t2 := v0 * t1
RETURN t2

FUNCTION main :
READ v1
IF v1 > #1 GOTO label3
GOTO label4
LABEL label3 :
ARG v1
v2 := CALL fact
GOTO label5
LABEL label4 :
v2 := #1
LABEL label5 :
WRITE v2
RETURN #0